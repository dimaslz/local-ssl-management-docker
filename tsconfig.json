{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		// "resolveJsonModule": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"target": "ESNext",
		"paths": {
			"*": ["node_modules/*", "src/types/*"],
			"@/*": ["src/*"],
			"~/*": ["./*"],
		}
	},
}
